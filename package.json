{
  "name": "metainfo2dts",
  "version": "1.0.0",
  "description": "create d.ts file(s) from nexacro metainfo file(s)",
  "main": "bin/index.js",
  "bin": {
    "nexacro-meta2dts": "./bin/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node bin/index.js",
