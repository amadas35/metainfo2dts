  },
  "keywords": [
    "nexacro",
    "metainfo",
    "typescript"
  ],
  "author": "TOBESOFT",
  "license": "ISC",
  "dependencies": {
    "commander": "^9.0.0",
    "glob": "^7.2.0",
    "xml2js": "^0.4.23"
  },
